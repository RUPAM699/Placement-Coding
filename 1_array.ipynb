    "def sunset_view(arr):\n",
    "    mx = arr[0]\n",
    "    count  = 1\n",
    "    for i in range(len(arr)):\n",
    "        if (arr[i]>mx):\n",
    "            count+=1\n",
    "            mx = arr[i]\n",
    "    print(count)\n",
    "arr = [12,15,26,21,13,11,27]\n",
    "sunset_view(arr) "
